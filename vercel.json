{
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/nicoalbanese/ai-sdk-slackbot"
    }
  ],
  "functions": {
    "api/events.ts": {
      "maxDuration": 60
    },
    "api/cron/ingest.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/cron/ingest",
      "schedule": "0 0 * * *"
    }
  ],
  "outputDirectory": "dist"
}
